<p id="notice"><%= notice %></p>

<h1>Listing Schools</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Ad first name</th>
      <th>Ad last name</th>
      <th>Ad phone</th>
      <th>Ad cell</th>
      <th>Ad extension</th>
      <th>Ad email</th>
      <th>Coach first name</th>
      <th>Coach last name</th>
      <th>Coach phone</th>
      <th>Coach cell</th>
      <th>Coach extension</th>
      <th>Coach email</th>
      <th>Dues paid</th>
      <th>Fees paid</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.name %></td>
        <td><%= school.address %></td>
        <td><%= school.phone %></td>
        <td><%= school.AD_first_name %></td>
        <td><%= school.AD_last_name %></td>
        <td><%= school.AD_phone %></td>
        <td><%= school.AD_cell %></td>
        <td><%= school.AD_extension %></td>
        <td><%= school.AD_email %></td>
        <td><%= school.coach_first_name %></td>
        <td><%= school.coach_last_name %></td>
        <td><%= school.coach_phone %></td>
        <td><%= school.coach_cell %></td>
        <td><%= school.coach_extension %></td>
        <td><%= school.coach_email %></td>
        <td><%= school.dues_paid %></td>
        <td><%= school.fees_paid %></td>
        <td><%= school.active %></td>
        <td><%= link_to 'Show', school %></td>
        <td><%= link_to 'Edit', edit_school_path(school) %></td>
        <td><%= link_to 'Destroy', school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School', new_school_path %>
